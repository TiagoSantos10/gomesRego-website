{
  "extends": "next/core-web-vitals",
    "rules": {
        "linebreak-style": 0,
        // enable additional rules
        "indent": ["error", 4],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "eol-last": ["error", "always"],
        "no-console": "error",
        "no-unused-vars": "error",
        "@next/next/no-img-element": "off"
    }
}
